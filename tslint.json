{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "jsx-double"],
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "semicolon": false,
        "interface-over-type-literal": false,
        "variable-name": {
            "options": ["allow-leading-underscore"]
        }
    },
    "rulesDirectory": []
}